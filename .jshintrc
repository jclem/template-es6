{
  "browser"      : true,
  "camelcase"    : true,
  "curly"        : true,
  "eqeqeq"       : true,
  "esnext"       : true,
  "forin"        : true,
  "freeze"       : true,
  "immed"        : true,
  "indent"       : 2,
  "maxcomplexity": 6,
  "maxdepth"     : 3,
  "maxlen"       : 80,
  "maxparams"    : 4,
  "maxstatements": 24,
  "newcap"       : true,
  "noarg"        : true,
  "noempty"      : true,
  "nonbsp"       : true,
  "nonew"        : true,
  "quotmark"     : "single",
  "undef"        : true,
  "unused"       : true
}
